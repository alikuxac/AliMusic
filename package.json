{
  "name": "@aliuxac/music",
  "version": "1.1.0",
  "description": "Alikuxac Personal Music Discord Bot",
  "main": "dist/index.js",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/structures/*": "./dist/lib/structures/*.js",
    "#config": "./dist/config/*.js",
    "#root/*": "./dist/*.js",
    "#models/*": "./dist/models/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#decorators/*": "./dist/decorators/*.js",
    "#constants/*": "./dist/constants/*.js"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -b src",
    "compile": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "pm2": "pm2 start npm --name AliMusic -- start --watch",
    "pm2:npx": "npx pm2 start npm --name AliMusic -- start --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alikuxac/AliMusic.git"
  },
  "keywords": [
    "discord",
    "alikuxac"
  ],
  "author": "Alikuxac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alikuxac/AliMusic/issues"
  },
  "homepage": "https://github.com/alikuxac/AliMusic#readme",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.10.0",
    "@distube/soundcloud": "^1.1.0",
    "@distube/spotify": "^1.1.0",
    "@sapphire/decorators": "^4.0.2",
    "@sapphire/discord-utilities": "^2.6.0",
    "@sapphire/discord.js-utilities": "^4.4.0",
    "@sapphire/framework": "^2.3.0",
    "@sapphire/pieces": "^3.2.0",
    "axios": "^0.27.2",
    "colorette": "^2.0.16",
    "discord.js": "^13.6.0",
    "distube": "^3.3.1",
    "dotenv": "^16.0.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.8",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.47",
    "node-abort-controller": "3.0.1",
    "rimraf": "3.0.2",
    "typescript": "4.7.4",
    "undici": "5.6.0"
  }
}
