{
  "name": "@aliuxac/music",
  "version": "1.2.0",
  "description": "Alikuxac Personal Music Discord Bot",
  "main": "dist/index.js",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/structures/*": "./dist/lib/structures/*.js",
    "#config": "./dist/config/*.js",
    "#root/*": "./dist/*.js",
    "#models/*": "./dist/models/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#decorators/*": "./dist/decorators/*.js",
    "#constants/*": "./dist/constants/*.js"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -b src",
    "compile": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "pm2": "pm2 start npm --name AliMusic -- start --watch",
    "pm2:npx": "npx pm2 start npm --name AliMusic -- start --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alikuxac/AliMusic.git"
  },
  "keywords": [
    "discord",
    "alikuxac"
  ],
  "author": "Alikuxac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alikuxac/AliMusic/issues"
  },
  "homepage": "https://github.com/alikuxac/AliMusic#readme",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@distube/soundcloud": "^2.0.2",
    "@distube/spotify": "^2.0.2",
    "@distube/yt-dlp": "^2.0.1",
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/discord-utilities": "^3.3.0",
    "@sapphire/discord.js-utilities": "^7.3.0",
    "@sapphire/framework": "^5.2.1",
    "@sapphire/pieces": "^4.3.1",
    "axios": "^1.7.2",
    "colorette": "^2.0.20",
    "discord.js": "^14.15.3",
    "distube": "^5.0.2",
    "dotenv": "^16.4.5",
    "libsodium-wrappers": "^0.7.13",
    "lodash": "^4.17.21",
    "mongoose": "^8.4.4",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@types/lodash": "4.17.6",
    "@types/node": "20.17.0",
    "node-abort-controller": "3.1.1",
    "rimraf": "5.0.7",
    "typescript": "5.5.3"
  }
}
